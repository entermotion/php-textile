<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor>
    <parser>
        <default-package-name>php-textile</default-package-name>
        <target>build/api-cache</target>
    </parser>
    <transformer>
        <target>build/docs</target>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>build/*</ignore>
        <ignore>docker/*</ignore>
        <ignore>test/*</ignore>
        <ignore>vendor/*</ignore>
    </files>
    <transformations>
        <template name="clean"/>
    </transformations>
</phpdocumentor>
